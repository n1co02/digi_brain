{
  "name": "gpt-service",
  "version": "0.0.1",
  "description": "",
  "main": "./src/main.ts",
  "scripts": {
    "lint-ts": "eslint  \"src/**/*.ts\" --max-warnings=0",
    "lint-ts-fix": "eslint  \"src/**/*.ts\" --fix",
    "lint": "npm run lint-ts",
    "lint-fix": "npm run lint-ts-fix",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx,ts}\"",
    "build-dependencies": "node scripts/buildResources.js",
    "build-swc": "swc src --out-dir dist",
    "build": "rimraf dist && npm run build-swc",
    "start-build": "node --expose-gc dist/main.js",
    "start": "concurrently -r \"npm:watch-* -s\"",
    "watch-start": "tsnd --transpile-only src/main.ts",
    "watch-tsc": "npm run lint-tsc -- --watch --preserveWatchOutput"
  },
  "author": "Erik Priemer",
  "devDependencies": {
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.7",
    "@types/node": "^18.11.5",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^7.1.0",
    "ts-loader": "^9.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.16",
    "@graphql-tools/load": "^7.8.12",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-graphql": "^0.9.0",
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "http": "^0.0.1-security",
    "openai": "^3.3.0",
    "sequelize": "^6.28.0"
  }
}
